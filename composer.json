{
    "name": "jalno/storage",
    "version": "0.0.1",
    "description": "Jalno Storage",
    "autoload": {
        "psr-4": {
            "Jalno\\Storage\\": "src"
        }
    },
    "require": {
        "jalno/lumen": "^0.0.1",
        "league/flysystem": "^1.1.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.7.6",
        "orchestra/testbench": "^6.18.0"
    },
    "scripts": {
        "test:types": "./vendor/phpstan/phpstan/phpstan analyse -vvv --debug --memory-limit 256M phpstan.neon src",
        "test": [
            "@test:types"
        ]
    },
    "extra": {
        "jalno": {
            "package": "Jalno\\Storage\\Package"
        }
    }
}
